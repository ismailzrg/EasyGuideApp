MDScreen:
    name: "pre_window"
    MDFloatLayout:
        md_bg_color: [1, 1, 1, 1]
    Image:
        source: "pharmicon.png"
        size_hint: .26, .26
        pos_hint: {'center_x': 0.5,'center_y': 0.55}
        canvas.before:
            Color:
                rgb: 1, 1, 1, 1
            Ellipse:
                size: 130, 130
                pos: 110, 250
    MDLabel:
        text: "EasyGuide"
        pos_hint: {'center_x': .5, 'center_y': .3}
        font_size: "36sp"
        halign: "center"
        theme_text_color: "Custom"
        text_color: 0, 0, 1, 1
    MDLabel:
        text: "Votre Chemin Parfait ..."
        pos_hint: {'center_x': .5, 'center_y': .23}
        halign: "center"
        theme_text_color: "Custom"
        text_color: 0, 0, 1, 1
        font_size: "18sp"
